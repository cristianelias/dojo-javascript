{
  "name": "dojo-javascript",
  "version": "1.0.0",
  "description": "Dojo javascript",
  "scripts": {
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha tests/**.spec.js -- --require babel-register",
    "test:watch": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha tests/**.spec.js -- --require babel-register --watch",
